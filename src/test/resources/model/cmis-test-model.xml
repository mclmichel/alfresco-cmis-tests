<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="swct:cmistestmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Cmis Test Model</description>
	<author>Springy Web</author>
	<version>1.0</version>
	
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
	</imports>

	<namespaces>
		<namespace uri="http://www.springyweb.com/model/cmistest/1.0" prefix="swct" />
	</namespaces>

	<types>
		<type name="swct:folder">
			<title>Test Folder</title>
			<parent>cm:folder</parent>
		</type>
		<type name="swct:document">
			<title>Test Document</title>
			<parent>cm:content</parent>
			<properties>
				<property name="swct:propSingleString">
					<type>d:text</type>
				</property>
				<property name="swct:propSingleInt">
					<type>d:int</type>
				</property>
				<property name="swct:propSingleDouble">
					<type>d:double</type>
				</property>
				<property name="swct:propSingleBoolean">
					<type>d:boolean</type>
				</property>
				<property name="swct:propSingleDate">
					<type>d:date</type>
				</property>
				<property name="swct:propSingleDateTime">
					<type>d:datetime</type>
				</property>
				<property name="swct:propMultipleString">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				<property name="swct:propMultipleInt">
					<type>d:int</type>
					<multiple>true</multiple>
				</property>
				<property name="swct:propMultipleDouble">
					<type>d:double</type>
					<multiple>true</multiple>
				</property>
				<property name="swct:propMultipleBoolean">
					<type>d:boolean</type>
					<multiple>true</multiple>
				</property>
				<property name="swct:propMultipleDate">
					<type>d:date</type>
					<multiple>true</multiple>
				</property>
				<property name="swct:propMultipleDateTime">
					<type>d:datetime</type>
					<multiple>true</multiple>
				</property>
			</properties>
		</type>
	</types>
</model>