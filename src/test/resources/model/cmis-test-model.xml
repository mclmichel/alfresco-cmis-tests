<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="swct:cmistestmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Cmis Test Model</description>
	<author>Springy Web</author>
	<version>1.0</version>
	
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>

	<namespaces>
		<namespace uri="http://www.springyweb.com/model/cmistest/1.0" prefix="swct" />
	</namespaces>

	<types>
		<type name="swct:document">
			<title>Cmis Test Document</title>
			<parent>cm:content</parent>
			<properties>
				<property name="swct:propSingleInt">
					<type>d:int</type>
				</property>
				<property name="swct:propSingleDouble">
					<type>d:double</type>
				</property>
				<property name="swct:propSingleBoolean">
					<type>d:boolean</type>
				</property>
				<property name="swct:propSingleDate">
					<type>d:date</type>
				</property>
				<property name="swct:propSingleDateTime">
					<type>d:datetime</type>
				</property>
			</properties>
			<associations>
				<association name="swct:assocManyToManyOpt">
					<title>Optional Many to Many Assoc</title>
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>swct:document</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
			<mandatory-aspects>
			  <aspect>cm:generalclassifiable</aspect>
			</mandatory-aspects>
		</type>
	</types>
</model>